        <script><![CDATA[function onLoad() {

    var levels = g_scratchpad.levels_count;
    var keys = {
        "1": "one",
        "2": "two",
        "3": "three",
        "4": "four",
        "5": "five"
    };
    var newLevels = {};

    for (var i in keys) {
        newLevels[keys[i]] = (i in levels && levels[i] > 1) ? false : true;
    }

	for (var key in newLevels) {

		g_form.setReadOnly("approval_rule_"+key, newLevels[key]);
	}


}]]></script>
